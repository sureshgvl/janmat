# Analytics - Staging (may use same or different tracking ID)
ENABLE_ANALYTICS=true
ANALYTICS_TRACKING_ID=GA_STAGING_ID

# Feature Flags - Test all features in staging
ENABLE_CHAT=true
ENABLE_VIDEO_PROCESSING=true
ENABLE_ADMOB=true
ENABLE_NOTIFICATIONS=true
ENABLE_VOICE_MESSAGES=true
ENABLE_OFFLINE_MODE=true

# Logging - Balanced verbosity for staging testing
LOG_LEVEL=info
LOG_MAX_FILE_SIZE_MB=10
LOG_RETENTION_DAYS=14
LOG_FILE_ROTATION_ENABLED=true

# Cache Configuration - Moderate for staging
CACHE_MAX_AGE_HOURS=12
CACHE_MAX_SIZE_MB=150
CACHE_ENABLE_DISK_STORAGE=true

# Staging Settings - Some debugging enabled
ENABLE_DEV_TOOLS=true
ENABLE_MOCK_DATA=false
ENABLE_CONSOLE_LOGGING=false
ENABLE_NETWORK_INSPECTOR=false

# Database Settings - Staging version
ENABLE_LOCAL_DATABASE=true
DATABASE_VERSION=1

# Push Notifications - Staging channel
ENABLE_FCM=true
FCM_DEFAULT_CHANNEL=staging

# Security Settings - Moderate for staging
ENABLE_APP_CHECK=false
SECURITY_LEVEL=staging

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_SAMPLES_ENABLED=true
